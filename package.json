{
  "name": "kingdom-one-wp-theme",
  "version": "1.0.0",
  "description": "An all-new Wordpress theme for Kingdom One",
  "main": "wp-content/themes/k1-theme/src/index.js",
  "scripts": {
    "build": "wp-scripts build",
    "check-engines": "wp-scripts check-engines",
    "format": "wp-scripts format ./wp-content/themes/k1-theme",
    "lint:style": "wp-scripts lint-style './wp-content/themes/k1-theme/src/**/*.scss'",
    "fix:style": "wp-scripts lint-style './wp-content/themes/k1-theme/src/**/*.scss' --fix",
    "lint:js": "wp-scripts lint-js ./wp-content/themes/k1-theme/src",
    "lint:md:docs": "wp-scripts lint-md-docs ./wp-content/themes/k1-theme",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "packages-update": "wp-scripts packages-update",
    "start": "wp-scripts start"
  },
  "author": "Choctaw Nation of Oklahoma",
  "license": "ISC",
  "dependencies": {
    "@wordpress/scripts": "^26.1.0",
    "bootstrap": "^5.2.3",
    "swiper": "^9.2.3"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "animate.css": "^4.1.1",
    "webpack-bundle-analyzer": "^4.8.0"
  }
}